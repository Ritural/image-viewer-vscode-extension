{
 "name": "image-viewer-vscode-extension",
 "version": "1.0.0",
 "description": "A VSCode extension that helps with viewing and using images in code.",
 "author": "Kallan Jones",
 "license": "MIT",
 "main": "./out/extension.js",
 "scripts": {
  "vscode:prepublish": "npm run compile",
  "compile": "tsc -p ./",
  "lint": "tslint -p ./",
  "watch": "tsc -watch -p ./"
 },
 "activationEvents": [
  "onCommand:extension.imageInsert"
 ],
 "contributes": {
  "commands": [
   {
    "command": "extension.imageInsert",
    "title": "Image insert"
   }
  ]
 },
 "engines": {
  "vscode": "^1.34.0"
 },
 "devDependencies": {
  "@types/node": "^12.12.11",
  "@types/vscode": "^1.40.0",
  "typescript": "^3.7.2"
 },
 "categories": [
  "Other"
 ],
 "homepage": "https://github.com/Ritural/image-viewer-vscode-extension#readme",
 "bugs": {
  "url": "https://github.com/Ritural/image-viewer-vscode-extension/issues"
 },
 "repository": {
  "type": "git",
  "url": "git+ssh://git@github.com/Ritural/image-viewer-vscode-extension.git"
 }
}
