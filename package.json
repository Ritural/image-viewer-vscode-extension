{
 "name": "image-viewer-vscode-extension",
 "version": "1.0.0",
 "description": "A VSCode extension that helps with viewing and using images in code.",
 "author": "Kallan Jones",
 "license": "MIT",
 "main": "./out/extension.js",
 "scripts": {
  "vscode:prepublish": "npm run compile",
  "compile": "tsc -p ./",
  "lint": "eslint src --ext ts",
  "watch": "tsc -watch -p ./",
  "pretest": "npm run compile && npm run lint",
  "test": "node ./out/test/runTest.js"
 },
 "activationEvents": [
  "onCommand:extension.viewProjectImages"
 ],
 "contributes": {
  "commands": [
   {
    "command": "extension.viewProjectImages",
    "title": "View project images"
   }
  ]
 },
 "engines": {
  "vscode": "^1.43.0"
 },
 "devDependencies": {
  "@types/glob": "^7.1.1",
  "@types/mocha": "^7.0.1",
  "@types/node": "^12.11.7",
  "@types/vscode": "^1.43.0",
  "eslint": "^6.8.0",
  "@typescript-eslint/parser": "^2.18.0",
  "@typescript-eslint/eslint-plugin": "^2.18.0",
  "glob": "^7.1.6",
  "mocha": "^7.0.1",
  "typescript": "^3.7.5",
  "vscode-test": "^1.3.0"
 },
 "categories": [
  "Other"
 ],
 "homepage": "https://github.com/Ritural/image-viewer-vscode-extension#readme",
 "bugs": {
  "url": "https://github.com/Ritural/image-viewer-vscode-extension/issues"
 },
 "repository": {
  "type": "git",
  "url": "git+ssh://git@github.com/Ritural/image-viewer-vscode-extension.git"
 }
}
